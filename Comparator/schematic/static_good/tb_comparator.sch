C {devices/code.sym} 1670 -190 0 0 {name=STIMULI 
only_toplevel=true
place=end
spice_ignore=false

value="
** this experimental option enables mos model bin 
** selection based on W/NF instead of W
.option chgtol=4e-16 method=gear

.param VCC_VAR =10
.param VCCGAUSS = aunif(3.3, 0.033*VCC_VAR)
.param VCC = 'VCCGAUSS'
** use following line to remove VCC variations
*.param VCC = 3.3
.param VDLGAUSS = aunif(1.65, 1.65)
.param VDL = VDLGAUSS
* use following line to remove input common mode variations
*.param VDL =  0.2
.param TEMPGAUSS = agauss(40, 30, 1)
.option temp = 'TEMPGAUSS'
** use following line to remove temperature variations
*.option temp = 27
.param DELTA = 2m
.param LMIN = 2

*.option savecurrents
.control
  let mc_runs = 10
  option seed = 8
  let run = 1
  save all
  op
  write tb_comparator.raw
  reset
  set appendwrite
  dowhile run < = mc_runs
    save vinp vinm out1 out2 out vdd temperat
    tran 1u 10u
    write tb_comparator.raw
    let run = run + 1
    reset
  end

quit
.endc
"}
